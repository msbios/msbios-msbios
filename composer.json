{
  "name": "msbios/msbios",
  "description": "MSBios Library",
  "license": "MIT",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\": "tests/"
    }
  },
  "require": {
    "zendframework/zend-loader": "^2.5",
    "zendframework/zend-modulemanager": "^2.8"
  },
  "require-dev": {
    "msbios/test": "^1.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit --colors=always"
    ],
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push"
  },
  "bin": [
    "bin/msbios.sh"
  ]
}
